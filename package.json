    "name": "@xogeny/book-nextgen",
    "description": "A book generator that is based on Next.js and Blueprint.js",
    "version": "0.1.0",
    "main": "lib/index.js",
    "scripts": {
        "dev": "next",
        "build": "next build",
        "export": "next export",
        "alias_preview": "now alias https://nextgen-book.michaeltiller.now.sh mbe-preview.modelica.university",
        "alias_beta": "now alias https://nextgen-book.michaeltiller.now.sh beta.book.xogeny.com",
        "alias_release": "now alias https://nextgen-book.michaeltiller.now.sh mbe.modelica.university",
        "alias_original": "now alias https://nextgen-book.michaeltiller.now.sh book.xogeny.com",
        "start": "next start",
        "serve": "yarn build && yarn export && serve out",
        "index": "node buildIndex.js",
        "now-build": "next build && next export -o dist",
        "upload": "now",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xogeny/book-nextgen.git"
